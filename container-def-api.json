{
    "family": "api-family",
    "executionRoleArn": "arn:aws:iam::026079580296:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "api-family",
            "image": "143283514385.dkr.ecr.us-east-1.amazonaws.com/api:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "512",
    "memory": "1024"
}